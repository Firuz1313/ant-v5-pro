{
  "version": 2,
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "npm run install:all",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/not-supported"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
