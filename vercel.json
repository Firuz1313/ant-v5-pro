{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "frontend/dist",
  "installCommand": "npm run install:all",
  "framework": "vite",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/not-supported"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
